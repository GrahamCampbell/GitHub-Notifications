{
    "name": "graham-campbell/github-notifications",
    "description": "Reduce your notification burden on GitHub!",
    "type": "project",
    "keywords": ["github", "notifications", "notify", "GitHubNotifications", "GitHub Notifications", "GitHub-Notifications", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "composer/xdebug-handler": "^1.4",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "knplabs/github-api": "^3.0",
        "symfony/console": "^4.3.8 || ^5.0",
        "symfony/error-handler": "^4.4 || ^5.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0.4",
        "phpunit/phpunit": "^8.5.14"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\GitHubNotifications\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\GitHubNotifications\\": "tests/"
        }
    },
    "bin": [
        "bin/notifications"
    ],
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": "dist"
    },
    "extra": {
        "bin-dir": "bin"
    }
}

{
    "name": "graham-campbell/github-notifications",
    "description": "Reduce your notification burden on GitHub!",
    "type": "project",
    "keywords": ["github", "notifications", "notify", "GitHubNotifications", "GitHub Notifications", "GitHub-Notifications", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk"
        }
    ],
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "require": {
        "php": "^7.4.15 || ^8.0.2",
        "ext-json": "*",
        "composer/xdebug-handler": "^1.4 || ^2.0 || ^3.0",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.4",
        "knplabs/github-api": "^3.9",
        "symfony/console": "^4.4 || ^5.0 || ^6.0",
        "symfony/error-handler": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^4.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\GitHubNotifications\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\GitHubNotifications\\": "tests/"
        }
    },
    "bin": [
        "bin/notifications"
    ],
    "config": {
        "platform": {
            "php": "7.4.15"
        },
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "bin-dir": "bin"
    }
}

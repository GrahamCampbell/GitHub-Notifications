{
    "name": "graham-campbell/github-notifications",
    "description": "Reduce your notification burden on GitHub!",
    "type": "project",
    "keywords": ["github"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^4.0",
        "knplabs/github-api": "2.11.*",
        "php-http/guzzle6-adapter": "1.1.*",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\GitHubNotifications\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": "dist"
    },
    "bin": ["notifications"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
